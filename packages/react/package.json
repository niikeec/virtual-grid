{
  "name": "@virtual-grid/react",
  "version": "2.0.0",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "wait-on ../core/dist/index.mjs && tsup --watch",
    "clean": "rm -rf .turbo node_modules dist",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.0.1",
    "@virtual-grid/core": "^2.0.0",
    "@virtual-grid/shared": "^2.0.0",
    "react-intersection-observer": "^9.5.2",
    "use-deep-compare": "^1.2.1",
    "use-resize-observer": "^9.1.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@virtual-grid/eslint-config": "workspace:*",
    "@virtual-grid/tsconfig": "workspace:*",
    "@virtual-grid/tsup-config": "workspace:*",
    "react": "^18.2.0",
    "tsup": "7.2.0",
    "typescript": "^4.5.2",
    "wait-on": "^7.2.0"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://www.virtual-grid.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niikeec/virtual-grid.git"
  },
  "keywords": [
    "react",
    "virtual",
    "grid",
    "tanstack"
  ]
}
